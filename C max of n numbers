#include <stdio.h>

int main() {
    int n, num, max;

    printf("Enter the value of n: ");
    scanf("%d", &n);

    printf("Enter %d integer numbers:\n", n);
    scanf("%d", &max);

    for (int i = 1; i < n; i++) {
        scanf("%d", &num);
        if (num > max) {
            max = num;
        }
    }

    printf("The biggest number is: %d\n", max);

    return 0;
}
